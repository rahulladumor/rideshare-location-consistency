{
  "name": "rideshare-location-consistency",
  "version": "1.0.0",
  "description": "Real-time location consistency system for ride-sharing platforms using AWS CDKTF",
  "main": "src/main.ts",
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "deploy": "cdktf deploy",
    "deploy:auto": "cdktf deploy --auto-approve",
    "destroy": "cdktf destroy",
    "destroy:auto": "cdktf destroy --auto-approve",
    "simulate": "ts-node scripts/simulate-traffic.ts",
    "generate-diagrams": "python docs/diagrams/generate_diagrams.py",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "clean": "rm -rf cdktf.out node_modules"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "aws",
    "cdktf",
    "terraform",
    "infrastructure-as-code",
    "iac",
    "location-services",
    "geospatial",
    "real-time",
    "ride-sharing",
    "multi-region",
    "dynamodb",
    "elasticache",
    "neptune",
    "iot-core",
    "lambda",
    "kinesis",
    "step-functions"
  ],
  "author": "Rahul Ladumor",
  "license": "MIT",
  "dependencies": {
    "@cdktf/provider-aws": "^19.0.0",
    "cdktf": "^0.20.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "aws-sdk": "^2.1400.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rahulladumor/rideshare-location-consistency.git"
  },
  "bugs": {
    "url": "https://github.com/rahulladumor/rideshare-location-consistency/issues"
  },
  "homepage": "https://github.com/rahulladumor/rideshare-location-consistency#readme"
}
